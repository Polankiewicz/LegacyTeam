;je¿eli w gracz info znajdziemy gracza po asercji, co ma równo pincet woja to daje znaæ o tym


(defrule isOkay
	(graczInfo(iloscWoja ?a&:(> ?a 400)))
	=>
	(assert (konkluzja(tekst "znaleziono")))
)

(defrule hasMore
	(graczInfo (iloscWoja ?iloscwoja))
	(test (< ?iloscwoja 400))
=>
	(printout t " znaleziono " crlf)
)